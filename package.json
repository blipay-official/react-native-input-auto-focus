{
  "name": "react-native-input-auto-focus",
  "version": "1.0.2",
  "description": "Auto focus for React Native TextInput",
  "main": "dist/module/index.js",
  "react-native": "dist/module/index.js",
  "types": "dist/typescript/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "lint": "tsc --noEmit && eslint ./",
    "prepare": "bob build",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --collectCoverage --runInBand --forceExit"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "text",
    "input",
    "focus"
  ],
  "repository": "https://github.com/blipay-official/react-native-input-auto-focus",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@eslint/compat": "^2.0.2",
    "@react-native-community/bob": "^0.17.1",
    "@react-native-community/eslint-config": "^3.2.0",
    "@react-navigation/native": "^7.1.28",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.2.14",
    "@types/react-native": "~0.67.0",
    "@types/react-test-renderer": "^19.1.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^9.39.2",
    "eslint-plugin-ft-flow": "^2.0.1",
    "jest": "^30.2.0",
    "jest-circus": "^30.2.0",
    "prettier": "^2.8.8",
    "react": "^17.0.2",
    "react-native": "^0.67.1",
    "react-test-renderer": "^17.0.2",
    "typescript": "~5.9.3"
  },
  "peerDependencies": {
    "react": ">=17.0.2",
    "react-native": ">=0.67.1"
  },
  "@react-native-community/bob": {
    "source": "src",
    "output": "dist",
    "targets": [
      "module",
      "typescript"
    ]
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-native(-community)?|react-navigation)/)"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/lib/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
