{
  "name": "react-native-input-auto-focus",
  "version": "1.0.0",
  "description": "Auto focus for React Native TextInput",
  "main": "dist/module/index.js",
  "react-native": "dist/module/index.js",
  "types": "dist/typescript/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "lint": "tsc --noEmit && eslint --ext .js,.jsx,.ts,.tsx ./",
    "prepare": "bob build",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --collectCoverage --detectOpenHandles --runInBand --forceExit"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "text",
    "input",
    "focus"
  ],
  "repository": "https://github.com/blipay-official/react-native-input-auto-focus",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@react-native-community/bob": "^0.16.2",
    "@react-native-community/eslint-config": "^3.0.1",
    "@react-navigation/native": "^6.0.10",
    "@testing-library/jest-native": "^4.0.4",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/react-native": "^9.1.0",
    "@types/jest": "^26.0.23",
    "@types/react": "~16.9.41",
    "@types/react-native": "~0.62.13",
    "@types/react-test-renderer": "^16.9.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.7.0",
    "jest": "^26.6.3",
    "jest-circus": "^27.5.1",
    "prettier": "^2.0.5",
    "react": "^17.0.2",
    "react-native": "^0.67.1",
    "react-test-renderer": "17.0.2",
    "typescript": "~3.9.5"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-native": "^0.67.1"
  },
  "@react-native-community/bob": {
    "source": "src",
    "output": "dist",
    "targets": [
      "module",
      "typescript"
    ]
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|react-navigation)/"
    ],
    "modulePathIgnorePatterns": ["<rootDir>/lib/"],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
